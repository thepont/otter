# Add and Commit
alias gc='git add -A :/ && git commit -am'


#--------------------------------------------------------------------------
# oh-my-zsh
#--------------------------------------------------------------------------

# Edit Config
alias zc="$EDITOR ~/.zshrc"

# Reload Config
alias zr=". ~/.zshrc"

# Open ohmyzsh folder
alias ohmyzsh="$EDITOR ~/.oh-my-zsh"


